{
    "apiVersion": "extensions/v1beta1",
    "kind": "Ingress",
    "metadata": {
        "name": "smeiling-ingress",
        "namespace": "default",
        "annotations": {
            "nginx.ingress.kubernetes.io/use-regex": "true"
        }
    },
    "spec": {
        "tls": [
            {
                "secretName": "smeiling-tls"
            }
        ],
        "rules": [
            {
                "path": "/alterdemo/*",
                "backend": {
                    "serviceName": "alterdemo",
                    "servicePort": "alterdemo"
                }
            },
            {
                "path": "/vodademo/*",
                "backend": {
                    "serviceName": "vodademo",
                    "servicePort": "vodademo"
                }
            }
        ]
    }
}